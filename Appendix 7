void forward(){  //forword
    digitalWrite(IN1, HIGH); //Right Motor MOVE
    digitalWrite(IN2, HIGH); //Left Motor MOVE
}

void turnRight(){ //turnRight
    digitalWrite(IN1, LOW);  //Right Motor STOP
    digitalWrite(IN2, HIGH); //Left Motor MOVE
}

void turnLeft(){ //turnLeft
    digitalWrite(IN1, HIGH); //Right Motor MOVE
    digitalWrite(IN2, LOW);  //Left Motor STOP
}

void Stop(){ //stop
    digitalWrite(in1, LOW); //Right Motor STOP
    digitalWrite(in4, LOW); //Left Motor STOP
}
